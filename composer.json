{
    "name": "otisz/laravel-billingo",
    "description": "Billingo API V3 integration for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "billingo",
        "api",
        "v3"
    ],
    "authors": [
        {
            "name": "Levente Otta",
            "email": "leventeotta@gmail.com",
            "homepage": "https://leventeotta.com"
        }
    ],
    "support": {
        "source": "https://github.com/Otisz/Laravel-Billingo",
        "issues": "https://github.com/Otisz/Laravel-Billingo/issues"
    },
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "illuminate/http": "^8.0",
        "guzzlehttp/guzzle": "^6.5|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Otisz\\Billingo\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Otisz\\Billingo\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Otisz\\Billingo\\BillingoServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "v3-dev"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
